<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id4"
         name="Headless Gama"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="msi.gama.headless.runtime.Application">
         </run>
      </application>
   </extension>
   <extension
         point="gaml.grammar.addition">
      <path
            name="gaml">
      </path>
   </extension>

</plugin>
